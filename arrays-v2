Algoritmo ArraysV2
    Dimension vector[20]
    positivo = 0
    negativo = 0
    nulo = 0
    Para f = 1 hasta 20 con paso 1 hacer			
        Escribir "Ingrese NÃºmero ", f, ": "		
        Leer vector[f]				
    FinPara		
    Para f = 1 hasta 20 con paso 1 hacer						
        Si vector[f] = 0 Entonces
            nulo = nulo + 1
        SiNo
            Si vector[f] > 0 Entonces
                positivo = positivo + 1
            SiNo
                negativo = negativo + 1
            Fin Si
        Fin Si		
    FinPara
    Escribir "Total de Nulos: ", nulo
    Escribir "Total de Positivos: ", positivo
    Escribir "Total de Negativos: ", negativo
FinAlgoritmo
